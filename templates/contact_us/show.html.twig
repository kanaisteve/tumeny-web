<!DOCTYPE html>

<title>ContactUs</title>

{% block body %}
    <h1>ContactUs</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact_u.id }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ contact_u.firstName }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ contact_u.lastName }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ contact_u.name }}</td>
            </tr>
            <tr>
                <th>MobileNumber</th>
                <td>{{ contact_u.mobileNumber }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ contact_u.email }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ contact_u.subject }}</td>
            </tr>
            <tr>
                <th>Body</th>
                <td>{{ contact_u.body }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ contact_u.status }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_contact_us_index') }}">back to list</a>

    <a href="{{ path('app_contact_us_edit', {'id': contact_u.id}) }}">edit</a>

    {{ include('contact_us/_delete_form.html.twig') }}
{% endblock %}
